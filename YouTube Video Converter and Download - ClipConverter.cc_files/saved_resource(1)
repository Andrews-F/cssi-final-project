function load_header(){createDropDown();var $dropTrigger=$(".dropdown dt a");var $languageList=$(".dropdown dd ul");$dropTrigger.toggle(function(){var source=$("#country-options");if($languageList.length==0){$("#target").append('<dd><ul></ul></dd>')
